// User Service Database Schema
generator client {
  provider = "prisma-client-js"
  output   = "../../../libs/generated/user-prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String   @id @default(uuid())
  email     String   @unique
  password  String
  name      String?
  createdAt DateTime @default(now())
}

